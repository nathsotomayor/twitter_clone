<div class="container pt-4">
  <div class="row">
    <div class="col">
      <%= render partial: "shared/navbar_lateral" %>
    </div>
    <div class="col-7">
      <%= render "profile", locals: {user: @user} %>
      <%= render "feed" %><br/>
      <div class="pagination justify-content-center">
        <%= will_paginate @tweets %>
      </div>
    </div>
    <div class="col-3">
      <%= render "search" %>
      <%= render "who_to_follow" %>
    </div>
  </div>
</div>
